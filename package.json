{
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@project-serum/serum": "^0.13.60",
    "@solana/spl-token": "^0.1.8",
    "@solana/spl-token-registry": "^0.2.287",
    "@solana/wallet-adapter-base": "^0.7.0",
    "@solana/wallet-adapter-react": "^0.13.1",
    "@solana/wallet-adapter-wallets": "^0.11.1",
    "@solana/web3.js": "^1.30.2",
    "antd": "^4.16.13",
    "craco-less": "^1.20.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start": "PORT=8080 craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "deploy": "gh-pages -d build",
    "deploy:ar": "arweave deploy-dir build --key-file ",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "prepare": "husky install",
    "upgrade": "yarn-upgrade-all"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.15",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "yarn-upgrade-all": "^0.5.4"
  }
}
